{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "unifi-networkconf.schema.json",
  "title": "UniFi networkconf Configuration",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "dhcpd_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "dhcpguard_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "domain_name": {
      "type": "string"
    },
    "igmp_snooping": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "mdns_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "is_nat": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "purpose": {
      "type": "string",
      "enum": {
        "values": [
          "corporate",
          "guest",
          "remote-user-vpn",
          "site-vpn",
          "vlan-only",
          "vpn-client",
          "wan"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "ipv6_interface_type": {
      "type": "string",
      "enum": {
        "values": [
          "none",
          "pd",
          "single_network",
          "static"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "ipv6_setting_preference": {
      "type": "string",
      "enum": {
        "values": [
          "auto",
          "manual"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "ipv6_client_address_assignment": {
      "type": "string",
      "enum": {
        "values": [
          "dhcpv6",
          "slaac"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "dhcpdv6_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "ipv6_ra_preferred_lifetime": {
      "type": "integer"
    },
    "ipv6_ra_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "ipv6_ra_priority": {
      "type": "string",
      "enum": {
        "values": [
          "high",
          "low",
          "medium"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "dhcpdv6_dns_auto": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "dhcpdv6_leasetime": {
      "type": "integer"
    },
    "ipv6_pd_start": {
      "type": "string"
    },
    "ipv6_pd_stop": {
      "type": "string"
    },
    "dhcpdv6_start": {
      "type": "string"
    },
    "dhcpdv6_stop": {
      "type": "string"
    },
    "site_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "auto_scale_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf"
        ]
      }
    },
    "attr_no_delete": {
      "type": "boolean"
    },
    "attr_hidden_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "ip_subnet": {
      "type": "string",
      "pattern": "^[0-9.]+/[0-9]+$"
    },
    "name": {
      "type": "string"
    },
    "dhcpd_start": {
      "type": "string"
    },
    "dhcpd_stop": {
      "type": "string"
    },
    "vlan_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "NetworkConf",
          "RadiusProfile",
          "WlanConf"
        ]
      }
    },
    "external_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "networkgroup": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
