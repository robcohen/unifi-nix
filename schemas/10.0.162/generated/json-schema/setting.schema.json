{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "unifi-setting.schema.json",
  "title": "UniFi setting Configuration",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "key": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "hostname": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
