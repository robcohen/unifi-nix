{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "unifi-wlanconf.schema.json",
  "title": "UniFi wlanconf Configuration",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "attr_no_delete": {
      "type": "boolean"
    },
    "attr_no_edit": {
      "type": "boolean"
    },
    "attr_hidden_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "site_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "DpiApp",
          "DpiGroup",
          "FirewallRule",
          "NetworkConf",
          "PortForward",
          "Routing",
          "WlanConf"
        ]
      }
    },
    "security": {
      "type": "string",
      "enum": {
        "values": [
          "open",
          "osen",
          "wep",
          "wpaeap",
          "wpapsk"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "wpa_mode": {
      "type": "string",
      "enum": {
        "values": [
          "auto",
          "wpa1",
          "wpa2"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "wpa_enc": {
      "type": "string",
      "enum": {
        "values": [
          "auto",
          "ccmp",
          "ccmp-256",
          "gcmp",
          "gcmp-256"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "hide_ssid": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "element_adopt": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "name": {
      "type": "string"
    },
    "x_passphrase": {
      "type": "string"
    },
    "minrate_ng_enabled": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "WlanConf"
        ]
      }
    },
    "minrate_ng_data_rate_kbps": {
      "type": "integer"
    }
  },
  "additionalProperties": true
}
