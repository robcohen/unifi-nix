{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "unifi-dashboard.schema.json",
  "title": "UniFi dashboard Configuration",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "attr_hidden_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{24}$"
    },
    "attr_no_edit": {
      "type": "string"
    },
    "attr_no_delete": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "is_public": {
      "type": "boolean",
      "enum": {
        "values": [
          "false",
          "true"
        ],
        "collections": [
          "Dashboard"
        ]
      }
    }
  },
  "additionalProperties": true
}
