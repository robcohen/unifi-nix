<!DOCTYPE html><html lang="en"><head><meta charset="utf-8"><title>UniFi Network - Server status</title><meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"><base href="/manage/"><link href="angular/g8c7e6ab65/fonts/aura/fonts.css" rel="stylesheet"><link href="angular/g8c7e6ab65/fonts/ubnt-icon/style.css" rel="stylesheet"><link href="angular/g8c7e6ab65/css/app.css" rel="stylesheet"><link rel="apple-touch-icon-precomposed" href="angular/g8c7e6ab65/images/favicons/favicon-192.png?v=2"><meta name="msapplication-TileColor" content="#0193d7"><meta name="msapplication-TileImage" content="angular/g8c7e6ab65/images/favicons/favicon-192.png?v=2"><link rel="apple-touch-icon-precomposed" sizes="192x192" href="angular/g8c7e6ab65/images/favicons/favicon-192.png?v=2"><link rel="apple-touch-icon-precomposed" href="angular/g8c7e6ab65/images/favicons/favicon-64.png?v=2" sizes="64x64"><link rel="icon" href="angular/g8c7e6ab65/images/favicons/favicon-32.png?v=2" sizes="32x32"><link rel="icon" href="angular/g8c7e6ab65/images/favicons/favicon-16.png?v=2" sizes="16x16"></head><body><script type="text/javascript">function checkStatus () {
  var xhr = new XMLHttpRequest();
  xhr.addEventListener('load', function (event) {
    try {
      var response = JSON.parse(event.currentTarget.response);
      if (response.meta && response.meta.up) {
        window.location.reload();
      } else  if (response.meta && response.meta.app_context_status) {
        document.getElementById("appContextStatus").innerHTML = response.meta.app_context_status

        var messageElement = document.getElementById("appContextMessage");
        if (response.meta.app_context_message) {
          messageElement.innerHTML = response.meta.app_context_message;
          messageElement.style.display = '';
        } else {
          messageElement.style.innerHTML = '';
          messageElement.style.display = 'none';
        }
      } else if (response.meta && response.meta.app_context_error) {
        showError(response.meta.app_context_error);
      } else {
        document.getElementById("appContextStatus").innerHTML = 'Network application is starting up...';
        var serverElement = document.getElementById("appContextMessage");
        serverElement.innerHTML = 'Please wait a moment';
        serverElement.display = '';
      }
    }
    catch (e) {
      console.log(e);
    }
  }, false);
  xhr.addEventListener('error', function (jqXHR, status, error) {
    showError('Connection error. Please check your Network application process state.', error);
  }, false);

  var url = location.protocol + '//' + location.host + '/status';
  xhr.open('GET', url);
  xhr.send();
}

function showError(msg, error) {
  document.getElementById("appContextStatus").innerHTML = '<p>' + msg + '</p>';;
  document.getElementById("appInfoBox").className += ' appInfoBox--danger';
  document.getElementById("appInfoBoxIcon").className = document.getElementById("appInfoBoxIcon").className.replace(/ubnt-icon--info/ , 'ubnt-icon--alert');
  var messageElement = document.getElementById("appContextMessage");
  messageElement.style.display = 'none';
  messageElement.style.innerHTML = '';
  console.log(error);
  clearInterval(checkInterval);
}

var checkInterval = setInterval(function(){
  checkStatus();
}, 500);
</script><div class="appContainer appContainer--responsive appContainer--centered verticalRhythmTop"><div class="appRow--comfy"><div class="col--xs12"><div class="appInfoBox appInfoBox--responsive" id="appInfoBox"><div class="appInfoBox__icon icon ubnt-icon--info" id="appInfoBoxIcon"></div><div class="appInfoBox__content"><div class="appInfoBox__header"><p id="appContextStatus">Network application is starting up...</p><p><span id="appContextMessage"></span></p></div></div></div></div></div></div></body></html>